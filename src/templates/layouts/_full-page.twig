{% import "partials/_mixins.twig" as mixins %}
<!doctype html>
<html>
<head>
  <meta charset="utf-8"/>
  <title>{% if title %}{{ title }}{% else %}Template{% endif %}</title>
  <meta http-equiv="X-UA-Compatible" content="IE=Edge"/>
  <meta name="viewport" content="width=device-width, initial-scale=1"/>
  <meta name="theme-color" content="#fff"/>
  <meta name="format-detection" content="telephone=no"/>
  <link rel="stylesheet" media="all" href="css/app.css"/>
  {% include "../partials/_media-tags.twig" %}
  {% include "../partials/_preloader-styles.twig" %}
</head>
<body>

{% include "partials/_stable-content.twig" %}

<div id="barba-wrapper">
  <div class="barba-container" data-namespace="{{ namespace }}">
    <div id="fullpage">
      {% block content %}{% endblock %}
    </div>
  </div>
</div>

<script type="text/javascript" src="js/app.js"></script>

</body>
</html>
